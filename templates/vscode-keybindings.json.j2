// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "cmd+3",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "cmd+l cmd+l",
    "command": "editor.action.insertSnippet",
    "args": {
      "name": "clh"
    },
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+k",
    "command": "editor.debug.action.addLogPoint",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+l",
    "command": "workbench.debug.panel.action.clearReplAction"
  },
  {
    "key": "ctrl+shift+alt+cmd+]",
    "command": "workbench.action.terminal.focusNext"
  },
  {
    "key": "cmd+k cmd+;",
    "command": "editor.action.showHover",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+i",
    "command": "-editor.action.showHover",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+e cmd+e",
    "command": "workbench.debug.viewlet.action.enableAllBreakpoints"
  },
  {
    "key": "cmd+r cmd+r",
    "command": "workbench.debug.viewlet.action.removeAllBreakpoints"
  },
  {
    "key": "cmd+m",
    "command": "workbench.action.debug.continue",
    "when": "inDebugMode"
  },
  {
    "key": "f5",
    "command": "-workbench.action.debug.continue",
    "when": "inDebugMode"
  },
  {
    "key": "shift+cmd+w shift+cmd+w",
    "command": "editor.debug.action.selectionToWatch"
  },
  {
    "key": "cmd+; cmd+;",
    "command": "editor.debug.action.toggleBreakpoint",
    "when": "debuggersAvailable && editorTextFocus"
  },
  {
    "key": "f9",
    "command": "-editor.debug.action.toggleBreakpoint",
    "when": "debuggersAvailable && editorTextFocus"
  },
  {
    "key": "cmd+d cmd+d",
    "command": "workbench.debug.viewlet.action.toggleBreakpointsActivatedAction"
  },
  {
    "key": "ctrl+cmd+n",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "ctrl+shift+]",
    "command": "workbench.action.increaseViewSize"
  },
  {
    "key": "ctrl+shift+[",
    "command": "workbench.action.decreaseViewSize"
  },
  {
    "key": "alt+cmd+p",
    "command": "projectManager.listProjectsNewWindow"
  },
  {
    "key": "alt+cmd+p",
    "command": "-projectManager.listProjects"
  },
  {
    "key": "shift+cmd+a",
    "command": "workbench.action.tasks.runTask"
  },
  {
    "key": "ctrl+cmd+m",
    "command": "editor.action.marker.next",
    "when": "editorFocus"
  },
  {
    "key": "alt+f8",
    "command": "-editor.action.marker.next",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+space",
    "command": "-extension.vim_ctrl+space",
    "when": "editorTextFocus && vim.active && vim.use<C-space> && !inDebugRepl && vim.mode != 'Insert'"
  },
  {
    "key": "ctrl+.",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
  }
]
